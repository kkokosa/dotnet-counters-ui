<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ScottPlot="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
        xmlns:ui="clr-namespace:DotnetCountersUi"
        xmlns:views="clr-namespace:DotnetCountersUi.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="DotnetCountersUi.Views.MainWindow"
        WindowStartupLocation="CenterScreen"
        Title="dotnet-counters-ui">
  <StackPanel Background="White"
              HorizontalAlignment="Stretch"
              VerticalAlignment="Stretch">
    <views:CounterGraph
        GraphId="cpu-usage" Height="300"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch">
    </views:CounterGraph>
      <views:CounterGraph
          GraphId="alloc-rate" Height="300"
          HorizontalAlignment="Stretch"
          VerticalAlignment="Stretch">
      </views:CounterGraph>
  </StackPanel>
</Window>
