<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="180" Height="60"
        WindowStartupLocation="CenterScreen"
        Icon="/Assets/dcu_ico.ico"
        x:Class="DotnetCountersUi.Views.DeleteGraphDialog"
        Title="Delete graph">
    <Grid ColumnDefinitions="*" RowDefinitions="*, Auto" Margin="2">
        <TextBlock Grid.Row="0" Text="Delete graph?"/>
        <StackPanel Grid.Row="1" Margin="2" Orientation="Horizontal">
            <Button Content="Yes" Click="YesButton_OnClick"/>
            <Button Content="No" Click="NoButton_OnClick"/>
        </StackPanel>
    </Grid>
</Window>
